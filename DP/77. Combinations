class Solution {
public:
    void solve(int ind,int n,vector<vector<int>>& ans,vector<int>& temp,int k){
        //Base case
        if(k == 0){
            ans.push_back(temp);
            return;
        }
        if(ind > n){
            return;
        }
        
        //Exclude
        solve(ind+1,n,ans,temp,k--);

        //Include
        temp.push_back(ind);
        solve(ind+1,n,ans,temp,k--);
        temp.pop_back();                   //Backtracking
    }
    vector<vector<int>> combine(int n, int k) {
        vector<vector<int>> ans;
        vector<int> temp;
        solve(1,n,ans,temp,k);
        return ans;
    }
};
