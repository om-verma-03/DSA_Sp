class Solution{/
public:
    void solve(vector<int>& arr,int ind,int temp,vector<int>& ans){
        if(ind >= arr.size()){
            ans.push_back(temp);
            return;
        }
        
        solve(arr,ind+1,temp+arr[ind],ans);
        solve(arr,ind+1,temp,ans);
    }
    vector<int> subsetSums(vector<int> arr, int N){
        vector<int> ans;
        solve(arr,0,0,ans);
        return ans;
    }
};
